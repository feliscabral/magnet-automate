{
    "description": "üîéüè∑Ô∏è Search and Tag Plugin by Felis Cabral - Search case columns and tag results.",
    "version": 1.1,
    "schema":
    [

         {
              "name": "custom heading field",
              "description": "",
              "elementType": {
                "CustomFieldHeading": {
                  "value": "üîéüè∑Ô∏è Search and Tag Plugin by Felis Cabral - Search case columns and tag results."
                }
              }
         },
         {
              "name": "Enable Plugin",
              "variableName": "SAT_ENABLED",
              "required": true,
              "elementType": {
                "CustomFieldCheckbox": {
                  "defaultValue": true
              } }
         },
         {
              "name": "Output log.txt",
              "variableName": "SAT_OUTPUT_LOG",
              "required": true,
              "elementType": {
                "CustomFieldCheckbox": {
                  "defaultValue": true
              } }
         },
         {
              "name": "Create tag if no results are found",
              "variableName": "SAT_EMPTY_TAG",
              "required": true,
              "elementType": {
                "CustomFieldCheckbox": {
                  "defaultValue": true
              } }
         },
         {
              "name": "Exit with error code 1 if no results are found (produce failure)",
              "variableName": "SAT_NO_RESULT_FAIL",
              "required": true,
              "elementType": {
                "CustomFieldCheckbox": {
                  "defaultValue": false
              } }
         },
         {
              "name": "Case Path",
              "description": "",
              "variableName": "SAT_CASE_PATH",
               "required": true,
              "elementType": {
                   "CustomFieldInput": {
                   "hintText": "hint",
                   "defaultValue": "${OUTPUT_PATH}\\..\\AXIOM_Process",
                   "lineHeight": 1
              } }
         },
         {
              "name": "Tag Name",
              "description": "",
              "variableName": "SAT_TAG_NAME",
               "required": true,
              "elementType": {
                   "CustomFieldInput": {
                   "hintText": "Required. Example: Emails from ACME",
                   "defaultValue": "",
                   "lineHeight": 1
              } }
         },
         {
           "name": "custom heading field",
           "description": "",
           "elementType": {
             "CustomFieldHeading": {
               "value": "Use @value for Definition Value / search word (example: Emails From @value)"
             }
           }
         },
         {
              "name": "Tag Color",
              "description": "",
              "variableName": "SAT_TAG_COLOR",
              "required": true,
              "elementType": {
              "CustomFieldDropDownList": {
                   "defaultValue": "Green",
                   "hintText": "Choose a value.",
                        "enum": ["[random]", "Green", "Red", "Yellow", "Orange", "Violete", "Blue", "Light Blue", "Pink"]
              } }
         },
         {
              "name": "custom heading field",
              "description": "",
              "elementType": {
                "CustomFieldHeading": {
                  "value": "If tag name already exist the color will not change."
                }
              }
         },
         {
              "name": "Definition Name (field)",
              "description": "",
              "variableName": "SAT_DEFINITION_NAME",
              "required": true,
              "elementType": {
                   "CustomFieldInput": {
                   "hintText": "Required. Example: Sender Name",
                   "defaultValue": "",
                   "lineHeight": 1
              } }
         },
         {
              "name": "Operation",
              "description": "",
              "variableName": "SAT_OPERATION",
              "required": true,
              "elementType": {
              "CustomFieldDropDownList": {
                   "defaultValue": "contains",
                   "hintText": "Choose a value.",
                        "enum": ["begins with", "ends with", "contains", "not contains", "equals", "not equals"]
              } }
         },
         {
              "name": "Definition Value / search word(s)",
              "description": "",
              "variableName": "SAT_DEFINITION_VALUE",
              "required": true,
              "elementType": {
                   "CustomFieldInput": {
                   "hintText": "Required. Example: @acme.com, @acmecorp.com",
                   "defaultValue": "",
                   "lineHeight": 1
              } }
         },
         {
              "name": "custom heading field",
              "description": "",
